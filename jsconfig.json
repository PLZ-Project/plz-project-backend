{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2022",
    "baseUrl": ".",
    "paths": {
      "@lib/*": ["src/lib/*"],
      "@config/*": ["src/config/*"],
      "@helper/*": ["src/lib/helper/*"],
      "@provider/*": ["src/lib/provider/*"],
      "@connection/*": ["src/lib/connection/*"],
      "@dao/*": ["src/dao/*"],
      "@models/*": ["src/models/*"],
      "@routes/*": ["src/routes/*"],
      "@service/*": ["src/service/*"],
      "@controller/*": ["src/controller/*"],
      "@middleware/*": ["src/middleware/*"],
      "@authRequestDTO/*": ["src/dto/request/auth/*"],
      "@userRequestDTO/*": ["src/dto/request/user/*"],
      "@likeRequestDTO/*": ["src/dto/request/like/*"],
      "@emailRequestDTO/*": ["src/dto/request/email/*"],
      "@boardRequestDTO/*": ["src/dto/request/board/*"],
      "@articleRequestDTO/*": ["src/dto/request/article/*"],
      "@commentRequestDTO/*": ["src/dto/request/comment/*"],
      "@communityRequestDTO/*": ["src/dto/request/community/*"],
      "@notificationRequestDTO/*": ["src/dto/request/notification/*"],
      "@authResponseDTO/*": ["src/dto/response/auth/*"],
      "@userResponseDTO/*": ["src/dto/response/user/*"],
      "@likeResponseDTO/*": ["src/dto/response/like/*"],
      "@emailResponseDTO/*": ["src/dto/response/email/*"],
      "@boardResponseDTO/*": ["src/dto/response/board/*"],
      "@articleResponseDTO/*": ["src/dto/response/article/*"],
      "@commentResponseDTO/*": ["src/dto/response/comment/*"],
      "@communityResponseDTO/*": ["src/dto/response/community/*"],
      "@notificationResponseDTO/*": ["src/dto/response/notification/*"],
    }
  },
  "include": ["**/*"],
  "exclude": ["node_modules"]
}